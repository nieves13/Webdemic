   			<parameter>
   				<param_code></param_code>
   				<param_description>
   					Without any parameter, it displays processes for the current shell.
   				</param_description>
   			</parameter>
				
				<parameter>
   				<param_code>-e</param_code>
   				<param_description>
   					Display every active process on a Linux system in generic format.
   				</param_description>
   			</parameter>
				
				<example_of_use>
					<code_example> </code_example>
