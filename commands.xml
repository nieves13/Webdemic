<commands>

  <windows>

    <command>
   	  <code>cmd</code>
   		<description>
   		   This command will open Windows system symbols
   		</description>
    </command>

		<command>
			<code>taskmgr</code>
			<description>
				This command will open the Windows task manager
			</description>
		</command>

		<command>
			<code>mrt</code>
			<description>
      This command will open the Windows Malicious Software Removal Tool
			</description>
		</command>

    <command>
      <code>%programfiles%</code>
  	  <description>
        This command opens the program installation folder in the operating system
			</description>
    </command>

  </windows>

  <linux>
   	<command>
   		<code>ls</code>
   		<description>
   			It presents to you the contents of a particular directory.
   		</description>
   		<parameters>
			
   			<parameter>
   				 <param_code>-l</param_code>
   				 <param_description>
   					 To list the content as a detailed list.
   				 </param_description>
   			</parameter>

